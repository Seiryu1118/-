<div class="container">
 <div class="row">
  <div class="col-md-12">
   <h2 class="bg-light text-center w100">管理者ログイン</h2>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
     <table class="table table-borderless text-center">
      <tbody>
       <tr>
        <th>メールアドレス</th>
        <td><%= f.email_field :email, autofocus: true, autocomplete: "email", size:"40" %></td>
       </tr>
       <tr>
        <th>パスワード</th>
        <td><%= f.password_field :password, autocomplete: "current-password", size:"40" %></td>
       </tr>
      </tbody>
     </table>
     <div class="row justify-content-center example">
       <%= f.submit "ログイン",class:"btn btn-primary center-block" %>
     </div>
    <% end %>
  </div>
 </div>
</div>


